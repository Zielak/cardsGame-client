{
  "name": "multiplayer-game",
  "version": "0.0.3",
  "description": "just testing multiplayer game with server & client",
  "main": "index.js",
  "scripts": {
    "start": "webpack-dev-server",
    "precommit": "npm run lint",
    "lint": "./node_modules/.bin/eslint ./ -f visualstudio",
    "test": "cross-env BABEL_ENV=test ava",
    "test:cover": "cross-env BABEL_ENV=test nyc ava",
    "test:watch": "cross-env BABEL_ENV=test ava --watch"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "colyseus.js": "^0.9.14",
    "cross-env": "^5.2.0",
    "pixi.js": "^4.8.2",
    "transformation-matrix": "^1.14.1",
    "uuid": "^3.3.2"
  },
  "devDependencies": {
    "@types/node": "^10.9.4",
    "@types/pixi.js": "^4.8.0",
    "ava": "^0.25.0",
    "css-loader": "^1.0.0",
    "eventemitter3": "^3.1.0",
    "html-webpack-plugin": "^3.2.0",
    "husky": "^0.14.3",
    "nyc": "^13.0.1",
    "sass-loader": "^7.1.0",
    "sinon": "^6.2.0",
    "style-loader": "^0.23.0",
    "ts-loader": "^5.0.0",
    "typescript": "^3.0.3",
    "webpack": "^4.17.2",
    "webpack-cli": "^3.1.0",
    "webpack-dev-server": "^3.1.8"
  },
  "ava": {
    "require": [
      "babel-register"
    ],
    "babel": "inherit"
  },
  "nyc": {
    "include": [
      "src/**/*"
    ],
    "all": true,
    "reporter": [
      "html",
      "text-summary"
    ],
    "sourceMap": true,
    "report-dir": "./dist/coverage"
  }
}
