{
  "name": "multiplayer-game",
  "version": "0.0.3",
  "description": "just testing multiplayer game with server & client",
  "main": "index.js",
  "scripts": {
    "start": "webpack-dev-server",
    "precommit": "npm run lint",
    "lint": "./node_modules/.bin/eslint ./ -f visualstudio",
    "test": "cross-env BABEL_ENV=test ava",
    "test:cover": "cross-env BABEL_ENV=test nyc ava",
    "test:watch": "cross-env BABEL_ENV=test ava --watch"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "colyseus.js": "^0.9.5",
    "cross-env": "^5.1.6",
    "pixi.js": "^4.8.0",
    "transformation-matrix": "^1.10.0",
    "uuid": "^3.2.1"
  },
  "devDependencies": {
    "@types/node": "^10.3.2",
    "@types/pixi.js": "^4.7.3",
    "ava": "^0.25.0",
    "babel-core": "^6.26.3",
    "babel-loader": "^8.0.2",
    "babel-plugin-module-resolver": "^3.1.1",
    "babel-plugin-provide-modules": "^1.0.3",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "babel-register": "^6.26.0",
    "css-loader": "^1.0.0",
    "eventemitter3": "^3.1.0",
    "html-webpack-plugin": "^3.2.0",
    "husky": "^0.14.3",
    "node-sass": "^4.9.0",
    "nyc": "^13.0.1",
    "sass-loader": "^7.0.1",
    "schema-utils": "^1.0.0",
    "sinon": "^6.2.0",
    "style-loader": "^0.23.0",
    "ts-loader": "^5.0.0",
    "typescript": "^3.0.3",
    "webpack": "^4.10.0",
    "webpack-cli": "^3.0.3",
    "webpack-dev-server": "^3.1.4"
  },
  "ava": {
    "require": [
      "babel-register"
    ],
    "babel": "inherit"
  },
  "nyc": {
    "include": [
      "src/**/*"
    ],
    "all": true,
    "reporter": [
      "html",
      "text-summary"
    ],
    "sourceMap": true,
    "report-dir": "./dist/coverage"
  }
}
